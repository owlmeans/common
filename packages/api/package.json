{
  "name": "@owlmeans/api",
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "build": "tsc -b",
    "dev": "sleep 0 && nodemon -e ts,tsx,json --watch src --exec \"tsc -p ./tsconfig.json\"",
    "watch": "tsc -b -w --preserveWatchOutput --pretty"
  },
  "main": "build/index.js",
  "module": "build/index.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "require": "./build/index.js",
      "default": "./build/index.js",
      "module": "./build/index.js",
      "types": "./build/index.d.ts"
    }
  },
  "dependencies": {
    "@owlmeans/auth-common": "^0.1.1",
    "@owlmeans/client-config": "^0.1.1",
    "@owlmeans/client-route": "^0.1.1",
    "@owlmeans/config": "^0.1.1",
    "@owlmeans/context": "^0.1.1",
    "@owlmeans/error": "^0.1.1",
    "@owlmeans/module": "^0.1.1",
    "@owlmeans/route": "^0.1.1",
    "axios": "^1.7.7",
    "qs": "^6.13.0"
  },
  "devDependencies": {
    "@types/qs": "^6.9.16",
    "nodemon": "^3.1.11",
    "npm-check": "^6.0.1",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
