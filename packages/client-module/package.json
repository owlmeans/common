{
  "name": "@owlmeans/client-module",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "tsc -b",
    "dev": "sleep 36 && nodemon -e ts,tsx,json --watch src --exec \"tsc -p ./tsconfig.json\"",
    "watch": "tsc -b -w --preserveWatchOutput --pretty"
  },
  "main": "build/index.js",
  "module": "build/index.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "require": "./build/index.js",
      "default": "./build/index.js",
      "module": "./build/index.js",
      "types": "./build/index.d.ts"
    }
  },
  "dependencies": {
    "@owlmeans/api": "^0.1.0",
    "@owlmeans/client-context": "^0.1.0",
    "@owlmeans/client-route": "^0.1.0",
    "@owlmeans/context": "^0.1.0",
    "@owlmeans/error": "^0.1.0",
    "@owlmeans/module": "^0.1.0",
    "@owlmeans/route": "^0.1.0",
    "ajv": "^8.16.0",
    "ajv-formats": "^3.0.1",
    "qs": "^6.12.2"
  },
  "devDependencies": {
    "@types/qs": "^6",
    "nodemon": "^3.1.4",
    "typescript": "^5.4.5"
  }
}
