{
  "name": "@owlmeans/client-panel",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "tsc -b",
    "dev": "nodemon -e ts,tsx,json --watch src --exec \"tsc -p ./tsconfig.json\"",
    "watch": "tsc -b -w --preserveWatchOutput --pretty"
  },
  "main": "build/index.js",
  "module": "build/index.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "require": "./build/index.js",
      "default": "./build/index.js",
      "module": "./build/index.js",
      "types": "./build/index.d.ts"
    },
    "./auth": {
      "import": "./build/auth/index.js",
      "require": "./build/auth/index.js",
      "default": "./build/auth/index.js",
      "module": "./build/auth/index.js",
      "types": "./build/auth/index.d.ts"
    }
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@owlmeans/client": "^0.1.0",
    "@owlmeans/client-i18n": "^0.1.0",
    "@owlmeans/client-module": "^0.1.0",
    "@owlmeans/context": "^0.1.0"
  },
  "peerDependencies": {
    "@emotion/react": "*",
    "@emotion/styled": "*",
    "@mui/material": "*",
    "@owlmeans/auth": "*",
    "@owlmeans/client-auth": "*",
    "ajv": "*",
    "react": "*",
    "react-dom": "*",
    "react-hook-form": "*"
  },
  "peerDependenciesMeta": {
    "@owlmeans/auth": {
      "optional": true
    },
    "@owlmeans/client-auth": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "devDependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.15.21",
    "@owlmeans/auth": "^0.1.0",
    "@owlmeans/client-auth": "^0.1.0",
    "@types/react": "^18.3.3",
    "ajv": "^8.16.0",
    "nodemon": "^3.1.4",
    "react": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "typescript": "^5.4.5"
  }
}
