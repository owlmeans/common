{
  "name": "@owlmeans/web-flow",
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "build": "tsc -b",
    "dev": "sleep 366 && nodemon -e ts,tsx,json --watch src --exec \"tsc -p ./tsconfig.json\"",
    "watch": "tsc -b -w --preserveWatchOutput --pretty"
  },
  "main": "build/index.js",
