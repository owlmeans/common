{
  "name": "@owlmeans/web-oidc-rp",
  "version": "0.1.2",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc -b",
    "dev": "sleep 378 && nodemon -e ts,tsx,json --watch src --exec \"tsc -p ./tsconfig.json\"",
    "watch": "tsc -b -w --preserveWatchOutput --pretty"
  },
  "main": "build/index.js",
  "module": "build/index.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "require": "./build/index.js",
      "default": "./build/index.js",
      "module": "./build/index.js",
      "types": "./build/index.d.ts"
    },
    "./auth/plugins": {
      "import": "./build/auth/plugins/index.js",
      "require": "./build/auth/plugins/index.js",
      "default": "./build/auth/plugins/index.js",
      "module": "./build/auth/plugins/index.js",
      "types": "./build/auth/plugins/index.d.ts"
    }
  },
  "dependencies": {
    "@owlmeans/auth": "^0.1.2",
    "@owlmeans/basic-envelope": "^0.1.2",
    "@owlmeans/client": "^0.1.2",
    "@owlmeans/client-auth": "^0.1.2",
    "@owlmeans/client-flow": "^0.1.2",
    "@owlmeans/client-i18n": "^0.1.2",
    "@owlmeans/context": "^0.1.2",
    "@owlmeans/flow": "^0.1.2",
    "@owlmeans/module": "^0.1.2",
    "@owlmeans/oidc": "^0.1.2",
    "@owlmeans/resource": "^0.1.2",
    "@owlmeans/web-client": "^0.1.2",
    "@owlmeans/web-flow": "^0.1.2",
    "oidc-client-ts": "^3.1.0"
  },
  "peerDependencies": {
    "@mui/material": "*",
    "react": "*",
    "react-router": "^7.*"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "nodemon": "^3.1.11",
    "npm-check": "^6.0.1",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
