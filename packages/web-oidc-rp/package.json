{
  "name": "@owlmeans/web-oidc-rp",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "tsc -b",
    "dev": "sleep 40 && nodemon -e ts,tsx,json --watch src --exec \"tsc -p ./tsconfig.json\"",
    "watch": "tsc -b -w --preserveWatchOutput --pretty"
  },
  "main": "build/index.js",
  "module": "build/index.js",
  "types": "build/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "require": "./build/index.js",
      "default": "./build/index.js",
      "module": "./build/index.js",
      "types": "./build/index.d.ts"
    },
    "./plugins": {
      "import": "./build/plugins/index.js",
      "require": "./build/plugins/index.js",
      "default": "./build/plugins/index.js",
      "module": "./build/plugins/index.js",
      "types": "./build/plugins/index.d.ts"
    }
  },
  "dependencies": {
    "@owlmeans/basic-envelope": "^0.1.0",
    "@owlmeans/client-auth": "^0.1.0",
    "@owlmeans/client-flow": "^0.1.0",
    "@owlmeans/context": "^0.1.0",
    "@owlmeans/oidc": "^0.1.0",
    "@owlmeans/web-client": "^0.1.0",
    "oidc-client-ts": "^3.0.1"
  },
  "peerDependencies": {
    "@mui/material": "*",
    "react": "*"
  },
  "devDependencies": {
    "@types/react": "^18.3.4",
    "nodemon": "^3.1.4",
    "typescript": "^5.5.4"
  }
}
